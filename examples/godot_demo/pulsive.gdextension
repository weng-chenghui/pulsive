[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.2
reloadable = true

[libraries]
macos.debug = "res://bin/libpulsive_godot.dylib"
macos.release = "res://bin/libpulsive_godot.dylib"
windows.debug.x86_64 = "res://bin/pulsive_godot.dll"
windows.release.x86_64 = "res://bin/pulsive_godot.dll"
linux.debug.x86_64 = "res://bin/libpulsive_godot.so"
linux.release.x86_64 = "res://bin/libpulsive_godot.so"

